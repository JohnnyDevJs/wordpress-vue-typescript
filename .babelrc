{
  "env": {
    "development": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": false,
            "targets": {
              "ie": "9"
            }
          }
        ]
      ],
      "plugins": [
        [
          "@babel/plugin-proposal-class-properties",
          {
            "loose": false
          }
        ]
      ]
    },
    "production": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": false,
            "forceAllTransforms": true,
            "targets": {
              "ie": "9"
            }
          }
        ]
      ]
    }
  }
}